<group name="Platform">
 <string id="name">
  Platform
 </string>
 <string id="category">
  Movements
 </string>
 <string id="description">
  Jump and run between platforms (solid objects).
 </string>
 <group name="properties">
  <string id="maxspeed">
   Max speed
  </string>
  <string id="maxspeed-desc">
   The maximum speed, in pixels per second, the object can accelerate to.
  </string>
  <string id="acceleration">
   Acceleration
  </string>
  <string id="acceleration-desc">
   The rate of acceleration, in pixels per second per second.
  </string>
  <string id="deceleration">
   Deceleration
  </string>
  <string id="deceleration-desc">
   The rate of deceleration, in pixels per second per second.
  </string>
  <string id="jumpstrength">
   Jump strength
  </string>
  <string id="jumpstrength-desc">
   Speed at which jumps start, in pixels per second.
  </string>
  <string id="gravity">
   Gravity
  </string>
  <string id="gravity-desc">
   Acceleration from gravity, in pixels per second per second.
  </string>
  <string id="maxfallspeed">
   Max fall speed
  </string>
  <string id="maxfallspeed-desc">
   Maximum speed object can reach in freefall, in pixels per second.
  </string>
  <string id="defaultcontrols">
   Default controls
  </string>
  <string id="defaultcontrols-desc">
   If enabled, arrow keys control movement.  Otherwise, use the 'simulate control' action.
  </string>
  <string id="defaultcontrols-options">
   No|Yes
  </string>
 </group>
