<group name="Car">
 <string id="name">
  Car
 </string>
 <string id="category">
  Movements
 </string>
 <string id="description">
  Moves an object forwards with steering.
 </string>
 <group name="properties">
  <string id="maxspeed">
   Max speed
  </string>
  <string id="maxspeed-desc">
   The maximum speed, in pixels per second, the object can travel at.
  </string>
  <string id="acceleration">
   Acceleration
  </string>
  <string id="acceleration-desc">
   The rate of acceleration, in pixels per second per second.
  </string>
  <string id="deceleration">
   Deceleration
  </string>
  <string id="deceleration-desc">
   The rate of deceleration, in pixels per second per second.
  </string>
  <string id="steerspeed">
   Steer speed
  </string>
  <string id="steerspeed-desc">
   The turn speed, in degrees per second.
  </string>
  <string id="driftrecover">
   Drift recover
  </string>
  <string id="driftrecover-desc">
   The rate the angle of motion catches up with the object angle.  Lower values cause drifting, higher values cause tighter turns.
  </string>
  <string id="friction">
   Friction
  </string>
  <string id="friction-desc">
   The amount of slowdown when hitting solids, between 0 and 1.
  </string>
  <string id="setangle">
   Set angle
  </string>
  <string id="setangle-desc">
   Set the object's angle while moving.
  </string>
  <string id="setangle-options">
   No|Yes
  </string>
  <string id="defaultcontrols">
   Default controls
  </string>
  <string id="defaultcontrols-desc">
   If enabled, arrow keys control movement.  Otherwise, use the 'simulate control' action.
  </string>
  <string id="defaultcontrols-options">
   No|Yes
  </string>
 </group>
