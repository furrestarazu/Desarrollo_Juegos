<group name="Fade">
 <string id="name">
  Desvanecer
 </string>
 <string id="category">
  General
 </string>
 <string id="description">
  Cambia la opacidad de un objeto en el tiempo. Usaro para desaparecer objetos gradualmente.
 </string>
 <group name="properties">
  <string id="fadeintime">
   Desvanecer en el tiempo
  </string>
  <string id="fadeintime-desc">
   Duración en segundos para el acentuado. Cero para saltar.
  </string>
  <string id="waittime">
   Espera
  </string>
  <string id="waittime-desc">
  Duración en segundos del tiempo entre el acentuado y el desvanecimiento. Cero para saltar.
  </string>
  <string id="fadeouttime">
   Desvanecimiento en el tiempo
  </string>
  <string id="fadeouttime-desc">
   Duración en segundos del desvanecimiento. Cero para saltar
  </string>
  <string id="destroy">
   Destruir
  </string>
  <string id="destroy-desc">
   Destruir el objeto luego de su desvanecimiento
  </string>
  <string id="destroy-options">
   No|Después del desvanecimiento
  </string>
 </group>
